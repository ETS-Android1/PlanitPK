<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LoginTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:textAllCaps">true</item>
        <item name="textAllCaps">true</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="circle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.MyApp.Button.Circle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.MyApp.Button.Rounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10%</item>
    </style>

    <style name="TopCorners">
        <item name="cornerSizeTopLeft">15dp</item>
        <item name="cornerSizeTopRight">15dp</item>
    </style>

    <style name="RoundedCorners">
        <item name="cornerSize">10dp</item>
    </style>

    <style name="IconOnlyButton">
        <item name="iconPadding">0dp</item>
        <item name="iconGravity">textStart</item>
    </style>

    <style name="GilroySemiBoldTextAppearance">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="AppSearchView" parent="Widget.AppCompat.SearchView">
        <item name="android:textSize">16sp</item>
    </style>


</resources>